{
    "manifest_version": 3,
    "name": "Terp Rater",
    "version": "1.0",
    "description": "Rates your professors at UMD",
    "icons": {
        "16" : "images/favicon-16.png",
        "32" : "images/favicon-32.png"
    },
    "content_scripts": [
        {
            "matches": ["https://app.testudo.umd.edu/soc/*"],
            "css": ["css/global.css", "css/tooltip.css", "css/modal.css", "css/courseUtils.css"],
            "js": ["js/tooltip.js", "js/modal.js", "js/courseUtils.js","js/content.js"]
        }
    ],
    "host_permissions": ["https://planetterp.com/api/v1/*"],
    "web_accessible_resources": [
        {
            "resources": [ "assets/*.ttf" ],
            "matches": [ "https://app.testudo.umd.edu/*" ]
        },
        {
            "resources": [ "assets/*.svg" ],
            "matches": [ "https://app.testudo.umd.edu/*" ]
        },
        {
            "resources": [ "assets/*.png" ],
            "matches": [ "https://app.testudo.umd.edu/*" ]
        }
      ]
}